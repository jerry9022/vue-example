{
  "name": "vue-demo",
  "version": "1.0.0",
  "description": "A base vue project with webpack",
  "main": "index.js",
  "scripts": {
    "init": "webpack --progress --config webpack.dev.config.js",
    "dev": "webpack-dev-server --inline --hot --compress --history-api-fallback --config webpack.dev.config.js",
    "build": "webpack --progress --hide-modules --config webpack.prod.config.js",
    "sdev": "webpack --progress --hide-modules --config webpack.sdev.config.js",
    "server": "node ../vue-demo-server/bin/www"
  },
  "author": "jerry",
  "license": "MIT",
  "dependencies": {
    "vue": "^1.0.25",
    "vue-resource": "^1.0.3",
    "vue-router": "^0.7.13",
    "vux": "^0.1.3",
    "weui": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^6.3.13",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-runtime": "^5.8.34",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.3",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^2.22.0",
    "http-proxy-middleware": "^0.17.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.16.2",
    "moment": "^2.15.1",
    "node-uuid": "^1.4.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.2.0",
    "vue-loader": "^7.2.0",
    "vue-resource": "^1.0.3",
    "vue-style-loader": "^1.0.0",
    "vuex": "^1.0.0-rc.2",
    "vux": "^0.1.3",
    "webpack": "^1.13.1",
    "weui": "^1.0.1"
  }
